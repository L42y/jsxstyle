{
  "name": "jsxstyle-loader",
  "version": "1.0.0-beta.3",
  "description": "A webpack loader that extracts out static styles defined on jsxstyle components",
  "author": "Mike Meyer",
  "license": "Apache-2.0",
  "main": "src/index.js",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "lint": "eslint {src,tests}/",
    "jest": "jest --verbose",
    "prettier": "prettier --single-quote --trailing-comma es5 --write '{src,tests}/**/*.js'",
    "ls_module": "LOADER_TMPFILE=$(npm --silent pack) && tar -tf $LOADER_TMPFILE && echo && rm -i $LOADER_TMPFILE",
    "prepare": "npm run lint"
  },
  "dependencies": {
    "babel-generator": "^6.24.1",
    "babel-traverse": "^6.24.1",
    "babel-types": "^6.24.1",
    "babylon": "^6.17.0",
    "invariant": "^2.2.1",
    "jsxstyle": "^2.0.0-beta.4",
    "loader-utils": "^1.1.0"
  },
  "devDependencies": {
    "eslint": "^4.3.0",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-node": "^5.1.1",
    "jest": "^20.0.4",
    "prettier": "^1.5.3",
    "react": "^15.4.2"
  }
}
