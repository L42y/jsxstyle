{
  "name": "jsx-dot-style",
  "version": "0.0.0",
  "description": "Static site for jsxstyle",
  "private": true,
  "author": "Mike Meyer",
  "license": "Apache-2.0",
  "homepage": "https://jsx.style",
  "main": "index.js",
  "scripts": {
    "build": "rimraf public && gatsby build",
    "deploy": "rimraf public && gatsby build && gh-pages -d public",
    "fetch-schema": "apollo-codegen introspect-schema http://localhost:8000/___graphql --output codegen/schema.json",
    "generate-gql-types": "apollo-codegen generate **/*.gql --target typescript-modern --add-typename --schema codegen/schema.json",
    "serve": "gatsby serve",
    "start": "gatsby develop"
  },
  "dependencies": {
    "@meyer/apollo-codegen": "^0.19.4",
    "@types/node": "^10.0.4",
    "@types/react": "^16.3.13",
    "@types/react-dom": "^16.0.5",
    "@types/webpack": "^4.1.4",
    "@types/webpack-env": "^1.13.6",
    "gatsby": "next",
    "gatsby-cli": "next",
    "gatsby-plugin-jsxstyle": "0.0.2",
    "gatsby-plugin-react-helmet": "next",
    "gatsby-plugin-typescript": "next",
    "gh-pages": "^1.2.0",
    "jsxstyle": "^2.1.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-helmet": "^5.2.0",
    "rimraf": "^2.6.2",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
