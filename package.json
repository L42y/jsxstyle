{
  "name": "jsxstyle",
  "version": "2.0.0-beta.1",
  "description": "React component styler",
  "repository": "git@github.com:smyte/jsxstyle.git",
  "author": "Pete Hunt",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint src/",
    "prettier": "prettier --single-quote --print-width 120 --bracket-spacing false --jsx-bracket-same-line --trailing-comma es5 --write 'src/**/*.js'",
    "example": "cd example; rimraf build; webpack",
    "start": "cd example; ./serve.js",
    "babel": "rimraf lib; babel ./src --out-dir ./lib",
    "watch": "rimraf lib; babel --watch ./src --out-dir ./lib",
    "prepublish": "npm run babel"
  },
  "dependencies": {
    "invariant": "^2.2.1",
    "prop-types": "^15.5.8"
  },
  "peerDependencies": {
    "react": ">=0.14.0",
    "react-dom": ">=0.14.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.0",
    "eslint": "^3.3.0",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^20.0.3",
    "prettier": "^1.1.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-hot-loader": "next",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.0",
    "temp": "^0.8.3",
    "webpack": "^2.6.0",
    "webpack-dev-server": "^2.4.5"
  }
}
