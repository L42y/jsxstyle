{
  "name": "jsxstyle",
  "version": "2.0.0-beta.5",
  "description": "React component styler",
  "repository": "git@github.com:smyte/jsxstyle.git",
  "author": "Pete Hunt",
  "license": "Apache-2.0",
  "main": "lib/jsxstyle.cjs.js",
  "module": "lib/jsxstyle.es.js",
  "jsnext:main": "lib/jsxstyle.es.js",
  "scripts": {
    "eslint": "eslint {src,tests,example,preact/src}/*.js",
    "jest": "jest --verbose",
    "karma": "karma start",
    "prepublishOnly": "npm run version_sync; npm run rollup",
    "prettier": "prettier --write {*,preact/*}/*.js",
    "ls_module": "LOADER_TMPFILE=$(npm --silent pack) && tar -tf $LOADER_TMPFILE && echo && rm -i $LOADER_TMPFILE",
    "rollup": "rimraf {preact/,}lib/*.{es,cjs}.js; rollup -c; prettier --write {preact/,}lib/*.{es,cjs}.js",
    "version_sync": "cd preact; npm version $npm_package_version",
    "start": "webpack-dev-server --hot --hotOnly --inline --historyApiFallback --config=example/webpack.config.js",
    "build": "rimraf example/build/*; webpack -p --progress --config=example/webpack.config.js",
    "ci": "npm run jest && npm run rollup && npm run karma"
  },
  "dependencies": {
    "invariant": "^2.2.1",
    "prop-types": "^15.5.8"
  },
  "peerDependencies": {
    "react": ">=0.14.0",
    "react-dom": ">=0.14.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^21.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.7",
    "dotenv": "^4.0.0",
    "eslint": "^4.6.1",
    "eslint-plugin-jest": "^21.0.2",
    "eslint-plugin-react": "^7.3.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^2.30.1",
    "jasmine-core": "^2.8.0",
    "jest": "^21.0.2",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-sauce-launcher": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.4",
    "prettier": "^1.6.1",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "rimraf": "^2.6.2",
    "rollup": "^0.49.3",
    "rollup-plugin-babel": "^3.0.2",
    "style-loader": "^0.18.0",
    "webpack": "^3.5.6",
    "webpack-dev-server": "^2.7.1"
  },
  "prettier": {
    "single-quote": true,
    "trailing-comma": "es5"
  },
  "jest": {
    "transform": {
      ".js": "<rootDir>/tests/_babelTransform.js"
    }
  }
}
