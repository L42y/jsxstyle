{
  "name": "jsxstyle",
  "version": "1.0.2",
  "description": "React component styler",
  "main": "lib/index.js",
  "scripts": {
    "test": "jasmine-node tests/",
    "lint": "eslint src/",
    "prettier": "prettier --single-quote --print-width 120 --bracket-spacing false --jsx-bracket-same-line --trailing-comma es5 --write 'src/**/*.js'",
    "example": "cd example; rimraf build; webpack",
    "start": "cd example; ./serve.js",
    "exp": "cd experimental; rimraf build; webpack",
    "babel": "rimraf lib; babel ./src --out-dir ./lib",
    "watch": "rimraf lib; babel --watch ./src --out-dir ./lib",
    "prepublish": "npm run babel"
  },
  "author": "Pete Hunt",
  "license": "Apache-2.0",
  "dependencies": {
    "invariant": "^2.2.1",
    "react-css-property-operations": "^15.4.1"
  },
  "peerDependencies": {
    "react": ">=0.14.0",
    "react-dom": ">=0.14.0"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "css-loader": "^0.27.3",
    "eslint": "^3.3.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-react": "^6.10.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.28.0",
    "jasmine-node": "^1.14.5",
    "jasmine-pit": "^2.0.2",
    "prettier": "^0.22.0",
    "react": ">=0.14.0",
    "react-dom": ">=0.14.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "rimraf": "^2.6.1",
    "style-loader": "^0.13.2",
    "temp": "^0.8.3",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.3"
  },
  "repository": "git@github.com:smyte/jsxstyle.git"
}
