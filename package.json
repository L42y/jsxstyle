{
  "name": "jsxstyle-monorepo",
  "version": "2.0.0-beta.6",
  "description": "The monorepo for jsxstyle and tools built for jsxstyle",
  "main": ".eslintrc.js",
  "private": true,
  "directories": {
    "example": "examples"
  },
  "prettier": {
    "single-quote": true,
    "trailing-comma": "es5"
  },
  "dependencies": {
    "babel-eslint": "^8.0.0",
    "babel-jest": "^21.0.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-jest-hoist": "^21.2.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.7",
    "dotenv": "^4.0.0",
    "eslint": "^4.7.2",
    "eslint-plugin-jest": "^21.0.2",
    "eslint-plugin-react": "^7.3.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^2.30.1",
    "jasmine-core": "^2.8.0",
    "jest": "^21.0.2",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-sauce-launcher": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.4",
    "lerna": "^2.2.0",
    "prettier": "^1.6.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.2",
    "style-loader": "^0.18.0",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.7.1"
  },
  "scripts": {
    "ci": "npm run jest && npm run rollup && npm run karma",
    "eslint": "eslint {packages,examples}/{src,tests}/*.js",
    "jest": "jest --verbose",
    "karma": "karma start",
    "ls_module": "LOADER_TMPFILE=$(npm --silent pack) && tar -tf $LOADER_TMPFILE && echo && rm -i $LOADER_TMPFILE",
    "prepublishOnly": "npm run rollup",
    "prettier": "prettier --write {packages,examples}/{src,tests}/*.js",
    "rollup": "rimraf packages/jsxstyle{,-*}/lib/*.{es,cjs}.js; rollup -c; prettier --write packages/jsxstyle{,-*}/lib/*.{es,cjs}.js",
    "start": "webpack-dev-server --hot --hotOnly --inline --historyApiFallback --config=example/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smyte/jsxstyle.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/smyte/jsxstyle/issues"
  },
  "homepage": "https://github.com/smyte/jsxstyle#readme",
  "jest": {
    "transform": {
      ".js": "<rootDir>/misc/jestBabelTransform.js"
    }
  }
}
